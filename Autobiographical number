# AUTOBIOGRAPHICAL NUMBERS
#COUNT OF AUTOBIOGRAPHICAL DIGITS
n=int(input())
n=str(n)
ls=[]
for i in range(len(n)):
    count=0
    for  j in n:
        if int(j)==i:
          count+=1
    ls.append(count)
l=[]
for i in range(len(n)):
    print(ls[i],n[i])
    if ls[i]==int(n[i]):
        l.append(1)
print(len(l))
if len(l)==len(n):
    print("given number  is a autobiographical number "
else :
    print("given number is not a auto biographical number ")

